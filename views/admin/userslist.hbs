<div class="row">
    <table class="table table-dark">
        <thead>
        <tr>
            <th scope="col">E-mail</th>
            <th scope="col">isAdmin</th>
        </tr>
        </thead>
        {{# each users}}
            <tbody>
                <tr>
                    <td>{{this.email}}</td>
                    <td>{{this.isAdmin}}</td>
                </tr>
            </tbody>
        {{/each}}
    </table>
    <div class="badge badge-info pull-right" id="totalUsers"> Users in total: {{length}}</div>
</div>